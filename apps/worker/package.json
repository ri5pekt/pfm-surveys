{
  "name": "worker",
  "version": "1.0.0",
  "private": true,
  "description": "Background worker for processing survey events and rollups",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "rollup:trigger": "tsx scripts/trigger-rollup.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "bullmq": "^5.1.0",
    "dotenv": "^16.4.1",
    "ioredis": "^5.3.2",
    "kysely": "^0.27.2",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
